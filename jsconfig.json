{
    "compilerOptions": {
        "target": "es2017",
        "allowSyntheticDefaultImports": false,
        "baseUrl": "./",
        "paths": {
            "Screens/*": [
                "src/components/screens/*"
            ],
            "HomeScreen": [
                "src/components/screens/HomeScreen/index.js"
            ],
            "Components/*": [
                "src/components/*"
            ],
            "App/*": [
                "src/*"
            ]
        }
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}